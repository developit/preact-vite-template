{
	"compileOnSave": false,
	"compilerOptions": {
		"jsx": "preserve",
		"jsxFactory": "preact.h",
		"jsxFragmentFactory": "preact.Fragment",
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"moduleResolution": "node",
		"target": "ES2017",
		"module": "esnext",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": false
	}
}
